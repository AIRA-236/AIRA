{
  "name": "aira",
  "version": "0.1.0",
  "description": "AI Agent Symbiotic Ecosystem - A revolutionary platform for AI agent collaboration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "ai",
    "agents",
    "blockchain",
    "machine-learning",
    "typescript"
  ],
  "author": "AIRA Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.0.0",
    "express": "^4.18.2",
    "typescript": "^4.9.5",
    "web3": "^1.9.0",
    "tensorflow.js": "^4.2.0",
    "ethers": "^6.0.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.24.7"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIRA-236/AIRA.git"
  },
  "bugs": {
    "url": "https://github.com/AIRA-236/AIRA/issues"
  },
  "homepage": "https://www.aira-agents.xyz",
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
